package com.monocept.classes;

public class employe {
	private String name;
	private String eid;
	private int salary;
	public employe(String name, String eid, int salary) {
		
		this.name = name;
		this.eid = eid;
		this.salary = salary;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEid() {
		return eid;
	}
	public void setEid(String eid) {
		this.eid = eid;
	}
	public int getSalary() {
		return salary;
	}
	public void setSalary(int salary) {
		this.salary = salary;
	}
	
}
